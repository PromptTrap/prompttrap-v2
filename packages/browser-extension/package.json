{
  "name": "@prompttrap/browser-extension",
  "version": "0.1.0",
  "description": "PromptTrap browser extension for AI usage monitoring",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
