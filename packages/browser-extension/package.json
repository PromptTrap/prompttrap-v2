  },
  "dependencies": {
    "@prompttrap/shared": "file:../shared"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.3",
    "vite": "^5.4.11",
    "vite-plugin-static-copy": "^1.0.6",
    "vitest": "^2.1.8"
  }
}
